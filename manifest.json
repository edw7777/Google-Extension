{
    "name": "Google Extension",
    "version": "1.0.0",
    "description": "Created by Edward Wang",
    "manifest_version": 3,
    "action":{
        "default_popup": "index.html"
    },

    "permissions": [
        "tabs",
        "webNavigation",
        "https://www.linkedin.com/"
    ],

    "background": {
        "service": [
            "extension.js"
        ]
    },

    "icons": {
         "16": "16.png",
           "48": "48.png",
          "128": "128.png"
    },

    "browser_action": {
        "default_popup": "popup.html"
      }
}